  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-horn:before {
  content: "\e614";
}

.icon-fankui:before {
  content: "\e60c";
}

.icon-chakan1:before {
  content: "\e66b";
}

.icon-qianjin:before {
  content: "\e62d";
}

.icon-xiala:before {
  content: "\e672";
}

.icon-dianzan:before {
  content: "\e61f";
}

.icon-jiankongshebei:before {
  content: "\e610";
}

.box {
  box-sizing: border-box;
}

.announcementInformation {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 14rpx 16rpx;
  border-bottom: 2rpx solid #eee;
  box-sizing: border-box;
  background-color: #fff;
  margin-bottom: 10rpx;
}

.title {
  align-items: center;
}

.titleText {
  font-size: 28rpx;
  font-weight: bold;
  align-items: center;
  color: #000;
}

.icon-horn {
  margin-left: 10rpx;
  font-size: 28rpx;
  align-items: center;
  color: #000;
}

.more {
  align-items: center;
  display: flex;
}

.moreText {
  color: #666;
  font-size: 22rpx;
  align-items: center;
}

.icon-qianjin {
  font-size: 22rpx;
  color: #666;
  align-items: center;
  margin-left: 4rpx;
}

.informationList {
  box-sizing: border-box;
  width: 100%;
}

.information {
  width: 100%;
  margin: 14rpx 0;
  display: flex;
  flex-direction: row;
  box-sizing: border-box;
  background-color: #fff;
  padding: 18rpx;
}

.information .image {
  width: 34%;
  height: 160rpx;
}

.information .image image {
  width: 100%;
  height: 100%;
  border-radius: 12rpx;
}
.information .image video {
  width: 100%;
  height: 100%;
  border-radius: 12rpx;
}
.titleTime {
  display: flex;
  justify-content: space-between;
  font-size: 26rpx;
}

.informationTitle {
  font-size: 24rpx;
  width: 100%;
  margin-bottom: 16rpx;
	font-weight: 700;
}

.informationContent {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 22rpx;
  width: 100%;
  margin-bottom: 16rpx;
}

.monitoringQuery {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 14rpx 16rpx;
  border-bottom: 2rpx solid #eee;
  box-sizing: border-box;
  margin-bottom: 14rpx;
  background-color: #fff;
}

.icon-jiankongshebei {
  margin-left: 10rpx;
  font-size: 28rpx;
  align-items: center;
  color: #000;
}

.monitorList {
  display: flex;
  width: 100%;
  flex-wrap: wrap;
  padding: 18rpx 0;
  box-sizing: border-box;
  background-color: #fff;
}

.monitor {
  margin: 0 18rpx 20rpx 18rpx;
  width: calc(50% - 36rpx);
}

.monitorImage {
  width: 100%;
}

.monitorImage image {
  width: 100%;
  height: 100%;
}

.monitorName {
  font-size: 26rpx;
  text-align: center;
  margin-top: 20rpx;
}

.newsBoxBottom {
  display: flex;
  flex-direction: row;
  font-size: 22rpx;
  align-items: center;
  justify-content: space-between;
  width: 100%;
}

.newsBox {
  width: 66%;
  margin-left: 24rpx;
}

.newsBoxBottomIconfont {
  margin-right: 10rpx;
  vertical-align: middle;
}

.number {
  vertical-align: middle;
}

.feedback {
  border-radius: 50%;
  width: 90rpx;
  height: 90rpx;
  line-height: 50rpx;
  text-align: center;
  position: fixed;
  bottom: 100rpx;
  right: 30rpx;
  background-color: #e64240;
  color: #fff;
  display: flex;
  box-sizing: border-box;
}
.icon-fankui{
  font-size: 48rpx;
  margin: auto;
}